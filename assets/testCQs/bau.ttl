                                          mof:Quality
                                        )
                          ] ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( mof:Artist
                                         mof:Conceptualization
                                         mof:Course
                                         mof:CreativeWork
                                         mof:Goal
                                         mof:NewlyFormedInstitution
                                         mof:Visualization
