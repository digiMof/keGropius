#################################################################
#    Object Properties
#################################################################

###  https://digimof.github.io/keGropius/ontoGropius#against
:against rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( :Academy
                                     :BauhausCourse
                                   )
                     ] ;
         rdfs:range :Error .


###  https://digimof.github.io/keGropius/ontoGropius#aimsAt
:aimsAt rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf owl:topObjectProperty ;
        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( :ArtisticMovement
                                    :Institution
                                  )
                    ] ;
        rdfs:range [ rdf:type owl:Class ;
                     owl:unionOf ( :CollaborativeWorkOfArt
                                   :Goal
                                 )
                   ] .


###  https://digimof.github.io/keGropius/ontoGropius#basisOf
:basisOf rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         owl:inverseOf :dependsOn ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( :Artist
                                     :Conceptualization
                                     :Course
                                     :CreativeWork
                                     :Goal
                                     :NewlyFormedInstitution
                                     :Visualization
                                   )
                     ] ;
         rdfs:range [ rdf:type owl:Class ;
                      owl:unionOf ( :Course
                                    :CreativeWork
                                    :Goal
                                    :NewlyFormedInstitution
                                    :Quality
                                  )
                    ] .


###  https://digimof.github.io/keGropius/ontoGropius#changedInto
:changedInto rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain :Academy ;
             rdfs:range :NewlyFormedInstitution .


###  https://digimof.github.io/keGropius/ontoGropius#dependsOn
:dependsOn rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Course
                                       :CreativeWork
                                       :Goal
                                       :NewlyFormedInstitution
                                       :Quality
                                     )
                       ] ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( :Artist
                                      :Conceptualization
                                      :Course
                                      :CreativeWork
                                      :Goal
                                      :NewlyFormedInstitution
                                      :Visualization
                                    )
                      ] .


###  https://digimof.github.io/keGropius/ontoGropius#embodiedIn
:embodiedIn rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :Idea ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( :CollaborativeWorkOfArt
                                       :Form
                                     )
                       ] .


###  https://digimof.github.io/keGropius/ontoGropius#emphasizedBy
:emphasizedBy rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              owl:inverseOf :emphasizes ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :Arts
                                          :Crafts
                                        )
